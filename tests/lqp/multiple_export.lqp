(transaction
  (epoch
    (writes
      (define
        (fragment :f1
          (def :cola1
            ([row::INT v::INT]
              (and
                (= row 1)
                (= 2 v))))
          (def :cola2
            ([row::INT v::STRING]
              (and
                (= row 1)
                (= v "hello"))))
          (def :colb1
            ([row_1::STRING row_2::(DECIMAL 18 6) row_3::UINT128 row_4::FLOAT v::FLOAT]
              (and
                (= row_1 "b")
                (primitive :rel_primitive_parse_decimal #64 #6 "12.12" row_2)
                (= row_3 0x123abc)
                (= row_4 1.234)
                (= v 6.1))))
          (def :colb2
            ([row_1::STRING row_2::(DECIMAL 18 6) row_3::UINT128 row_4::FLOAT v::INT]
              (and
                (= row_1 "b")
                (primitive :rel_primitive_parse_decimal #64 #6 "12.12" row_2)
                (= row_3 0x123abc)
                (= row_4 1.234)
                (= v 123)))))))

    (reads
      (export
        (export_csv_config
          (path "wrong")
          (columns (column "col1name" :cola1) (column "col2name" :cola2))
          { :partition_size 10
            :compression ""
            :syntax_quotechar "\""
            :syntax_escapechar "\\"}))
      (export
        (export_csv_config
          (path "wrong")
          (columns (column "blahblah" :colb1) (column "foofoo" :colb2))
          { :syntax_quotechar "\""
            :syntax_escapechar "\\"})))))
