(transaction
  (configure { :ivm.maintenance_level "off" :semantics_version 0})
  (epoch
    (writes
      (define
        (fragment
          :f1
          (def :lt_result ([x::INT y::INT] (and (= x 1) (< x y))))
          (def :le_result ([x::INT y::INT] (and (= x 2) (<= x y))))
          (def :gt_result ([x::INT y::INT] (and (= x 3) (> x y)))))))
    (reads
      (output :lt_result :lt_result)
      (output :le_result :le_result)
      (output :gt_result :gt_result))))
