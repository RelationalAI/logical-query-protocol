(transaction
  (configure { :ivm.maintenance_level "off" :semantics_version 0})
  (epoch
    (writes
      (define
        (fragment
          :f1
          (algorithm
            :R
            (script
              (loop
                (init
                  (assign
                    :R
                    ([a::INT b::INT] (or (and (= a 1) (= b 2)) (and (= a 2) (= b 3)))))
                  (assign :U ([a::INT b::INT] (and (= a 1) (= b 3))))
                  (assign :always ([] (false))))
                (script
                  (assign :always2 ([] (false)))
                  (loop
                    (init)
                    (script
                      (upsert :R ([a::INT b::INT] (atom :U a b)))
                      (break :always2 ([] (true)))))
                  (break :always ([] (true))))))))))
    (reads (output :R :R))))
