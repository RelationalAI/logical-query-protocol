(transaction
  (configure { :ivm.maintenance_level "off" :semantics_version 0})
  (epoch
    (writes
      (define
        (fragment :f1 (def :other ([v::INT] (+ 1 1 v))) (def :output ([v::INT] (= v 101))))))
    (reads (output :output_0 :output) (output :other_0 :other)))
