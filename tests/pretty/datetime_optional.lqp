(transaction
  (configure { :ivm.maintenance_level "off" :semantics_version 0})
  (epoch
    (writes
      (define
        (fragment
          :f1
          (def
            :dt
            ([v::DATETIME]
             (or (= v (datetime 2024 6 15 10 30 0 0)) (= v (datetime 2025 1 1 0 0 0 0))))))))
    (reads (output :dt :dt))))
