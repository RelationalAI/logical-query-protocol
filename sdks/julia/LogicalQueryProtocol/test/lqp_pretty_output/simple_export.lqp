(transaction
  (configure
    { :ivm.maintenance_level "off"
      :semantics_version 0})
  (epoch
    (writes
      (define
        (fragment
          :f1
          (def
            :col1
            ([row::INT
            v::INT] (and
              (=
                row
                1)
              (=
                2
                v))))
          (def
            :col2
            ([row::INT
            v::STRING] (and
              (=
                row
                1)
              (=
                v
                "hello")))))))
    (reads
      (export
        (export_csv_config
          (path
            "wrong")
          (columns
            (column
              "col1name"
              :col1)
            (column
              "col2name"
              :col2))
          { :compression ""
            :partition_size 10
            :syntax_delim ","
            :syntax_escapechar "\\"
            :syntax_header_row true
            :syntax_missing_string ""
            :syntax_quotechar "\""})))))
