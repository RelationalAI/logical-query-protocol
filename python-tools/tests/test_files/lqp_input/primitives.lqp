(transaction
  (epoch
    (local_writes
      (define
        (fragment :primitives
          (def :date
            ([v::DATE]
              (primitive :rel_primitive_parse_date "1998-12-01" "Y-m-d" v::DATE)))

          (def :decimal
            ([v::DECIMAL]
              (primitive :rel_primitive_parse_decimal #64 #2 "3.14" v::DECIMAL)))

          (def :day
            ([v::DAY]
              (primitive :rel_primitive_parse_day #90))))))

    (reads
      (output :date :date)
      (output :decimal :decimal)
      (output :day :day))))
