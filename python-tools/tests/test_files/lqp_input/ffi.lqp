(transaction
  (epoch
    (local_writes
      (define
        (fragment :primitives
          (def :numbers ([v::INT]
            (or
              (= v 5)
              (= v 12)
              (= v 3))))
          (def :sorted_numbers ([ord::INT v::INT]
            (ffi
              :rel_primitive_sort
              (args ([x::INT] (atom :numbers x)))
              (terms ord v)))))))

    (reads
      (output :numbers :numbers)
      (output :sorted_numbers :sorted_numbers))))
