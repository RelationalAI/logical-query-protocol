(transaction
  (epoch
    (local_writes
      (define
        (fragment :f1
          (def :decimal_64 ([plus::DECIMAL(64 4) minus::DECIMAL(64 4) mult::DECIMAL(64 4) div::DECIMAL(64 4)]
            (exists [a::DECIMAL(64 4) b::DECIMAL(64 4)]
              (and
                (relatom :dec_64_a a)
                (relatom :dec_64_b b)
                (+ a b plus)
                (- a b minus)
                (* a b mult)
                (/ a b div)))))

          (def :decimal_128 ([plus::DECIMAL(128 10) minus::DECIMAL(128 10) mult::DECIMAL(128 10) div::DECIMAL(128 10)]
            (exists [a::DECIMAL(128 10) b::DECIMAL(128 10)]
              (and
                (relatom :dec_128_a a)
                (relatom :dec_128_b b)
                (+ a b plus)
                (- a b minus)
                (* a b mult)
                (/ a b div))))))))

    (reads
      (output :dec_64 :decimal_64)
      (output :dec_128 :decimal_128))))
