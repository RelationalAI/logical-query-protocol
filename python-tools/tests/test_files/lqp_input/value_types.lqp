(transaction
  (epoch
    (local_writes
      (define
        (fragment :f1
          (def :output_decimal ([v::DECIMAL] (relatom :dec v)))
          (def :output_decimal64 ([v::DECIMAL64] (relatom :dec64 v)))
          (def :output_decimal128 ([v::DECIMAL128] (relatom :dec128 v)))
          (def :output_date ([v::DATE] (relatom :date v)))
          (def :output_datetime ([v::DATETIME] (relatom :datetime v)))
          (def :output_nanosecond ([v::NANOSECOND] (relatom :nanosecond v)))
          (def :output_microsecond ([v::MICROSECOND] (relatom :microsecond v)))
          (def :output_millisecond ([v::MILLISECOND] (relatom :millisecond v)))
          (def :output_second ([v::SECOND] (relatom :second v)))
          (def :output_minute ([v::MINUTE] (relatom :minute v)))
          (def :output_hour ([v::HOUR] (relatom :hour v)))
          (def :output_day ([v::DAY] (relatom :day v)))
          (def :output_week ([v::WEEK] (relatom :week v)))
          (def :output_month ([v::MONTH] (relatom :month v)))
          (def :output_year ([v::YEAR] (relatom :year v))))))

    (reads
      (output :dec :output_decimal)
      (output :dec64 :output_decimal64)
      (output :dec128 :output_decimal128)
      (output :date :output_date)
      (output :datetime :output_datetime)
      (output :nanosecond :output_nanosecond)
      (output :microsecond :output_microsecond)
      (output :millisecond :output_millisecond)
      (output :second :output_second)
      (output :minute :output_minute)
      (output :hour :output_hour)
      (output :day :output_day)
      (output :week :output_week)
      (output :month :output_month)
      (output :year :output_year))))
