(transaction
  (epoch
    (local_writes
      (define
        (fragment :f1
          (def :output_decimal ([v::DECIMAL] (relatom :dec v::DECIMAL)))
          (def :output_date ([v::DATE] (relatom :date v::DATE)))
          (def :output_datetime ([v::DATETIME] (relatom :datetime v::DATETIME)))
          (def :output_nanosecond ([v::NANOSECOND] (relatom :nanosecond v::NANOSECOND)))
          (def :output_microsecond ([v::MICROSECOND] (relatom :microsecond v::MICROSECOND)))
          (def :output_millisecond ([v::MILLISECOND] (relatom :millisecond v::MILLISECOND)))
          (def :output_second ([v::SECOND] (relatom :second v::SECOND)))
          (def :output_minute ([v::MINUTE] (relatom :minute v::MINUTE)))
          (def :output_hour ([v::HOUR] (relatom :hour v::HOUR)))
          (def :output_day ([v::DAY] (relatom :day v::DAY)))
          (def :output_week ([v::WEEK] (relatom :week v::WEEK)))
          (def :output_month ([v::MONTH] (relatom :month v::MONTH)))
          (def :output_year ([v::YEAR] (relatom :year v::YEAR))))))

    (reads
      (output :dec :output_decimal)
      (output :date :output_date)
      (output :datetime :output_datetime)
      (output :nanosecond :output_nanosecond)
      (output :microsecond :output_microsecond)
      (output :millisecond :output_millisecond)
      (output :second :output_second)
      (output :minute :output_minute)
      (output :hour :output_hour)
      (output :day :output_day)
      (output :week :output_week)
      (output :month :output_month)
      (output :year :output_year))))
