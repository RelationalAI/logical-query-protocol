(transaction
  (configure
    { :ivm.maintenance_level "off"
    :semantics_version 0})
  (epoch
    (writes
      (define
        (fragment
          :f2
          (def
            :hello
            ([v::INT] (=
              v
              101)))))
      (define
        (fragment
          :f1
          (def
            :output
            ([v::INT] (atom
              :hello
              v))))))
    (reads
      (output
        :epoch0
        :output)))
  (epoch
    (writes
      (undefine
        :f2))
    (reads
      (output
        :epoch1
        :output))))
