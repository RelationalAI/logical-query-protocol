(transaction
  (epoch
    (writes
      (define
        (fragment :f1
          (def :int_uint
            ([v::UINT128]
              (cast 123::INT v)))
          (def :int_int
            ([v::INT128]
              (cast 123::INT v)))
          (def :int_dec64
            ([v::(DECIMAL 18::INT 6::INT)]
              (cast 1234::INT v)))
          (def :dec64_int
            ([v::INT]
              (exists [d::(DECIMAL 18::INT 6::INT)]
                (and
                  (primitive :rel_primitive_decimal #64::INT #6::INT 7::INT d)
                  (cast d v)))))
          (def :int_dec128
            ([v::(DECIMAL 38::INT 10::INT)]
              (cast 12345::INT v)))
          (def :dec128_int
            ([v::INT]
              (exists [d::(DECIMAL 38::INT 10::INT)]
                (and
                  (primitive :rel_primitive_decimal #128::INT #10::INT 5::INT d)
                  (cast d v))))))))
    (reads
      (output :int_uint :int_uint)
      (output :int_int :int_int)
      (output :int_dec64 :int_dec64)
      (output :dec64_int :dec64_int)
      (output :int_dec128 :int_dec128)
      (output :dec128_int :dec128_int))))
