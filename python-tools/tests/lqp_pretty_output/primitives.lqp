(transaction
  (epoch
    (writes
      (define
        (fragment :primitives
          (def :date
            ([v::DATE]
              (primitive :rel_primitive_parse_date "1998-12-01"::STRING "Y-m-d"::STRING v)))
          (def :decimal
            ([v::(DECIMAL 18::INT 6::INT)]
              (primitive :rel_primitive_parse_decimal #64::INT #6::INT "3.14"::STRING v))))))
    (reads
      (output :date :date)
      (output :decimal :decimal))))
