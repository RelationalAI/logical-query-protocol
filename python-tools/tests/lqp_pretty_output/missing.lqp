(transaction
  (configure
    { :ivm.maintenance_level "off"
      :semantics_version 0})
  (epoch
    (writes
      (define
        (fragment :f1
          (def :miss1
            ([v::MISSING]
              (primitive :rel_primitive_eq missing v)))
          (def :miss2
            ([v::STRING]
              (primitive :rel_primitive_eq "missing" v))))))
    (reads
      (output :miss1 :miss1)
      (output :miss2 :miss2))))
