(transaction
  (configure
    { :ivm.maintenance_level "off"
      :semantics_version 0})
  (epoch
    (writes
      (define
        (fragment :f1
          (betree_relation :edb
            (betree_info
              (key_types INT)
              (value_types STRING INT)
              (betree_config 0.5 4 16 16)
              (betree_locator 0x1234567890123456789012 0 0)))
          (betree_relation :multi_key
            (betree_info
              (key_types INT STRING)
              (value_types FLOAT)
              (betree_config 0.8 8 32 32)
              (betree_locator 0xabcdef1234567890abcdef 100 2)))
          (betree_relation :numeric_types
            (betree_info
              (key_types UINT128)
              (value_types INT FLOAT (DECIMAL 18 4))
              (betree_config 0.3 10 20 64)
              (betree_locator 0xfedcba9876543210fedcba 500 3)))
          (betree_relation :temporal_data
            (betree_info
              (key_types DATE)
              (value_types DATETIME STRING)
              (betree_config 0.6 6 24 48)
              (betree_locator 0x9999888877776666555544 250 2)))
          (betree_relation :boolean_data
            (betree_info
              (key_types STRING)
              (value_types BOOLEAN MISSING)
              (betree_config 0.7 5 15 20)
              (betree_locator 0x1111222233334444555566 75 1)))
          (betree_relation :key_only
            (betree_info
              (key_types INT STRING)
              (value_types)
              (betree_config 0.4 12 18 40)
              (betree_locator 0x7777888899990000111122 10 1)))
          (betree_relation :path_simple
            (base_relation_path "my_base_relation" [INT STRING FLOAT]))
          (betree_relation :path_specialized
            (base_relation_path "another_path" [INT #"specialized_string" STRING]))
          (betree_relation :path_mixed
            (base_relation_path "complex_path" [(DECIMAL 10 2) INT #42 STRING]))
          (betree_relation :path_empty
            (base_relation_path "empty_path" []))
          (def :foo
            ([x::INT]
              (primitive :rel_primitive_eq x 1)))
          (algorithm :bar :baz
            (script
              (assign :bar
                ([y::INT]
                  (primitive :rel_primitive_eq y 2)))))))
      (define
        (fragment :f2
          (betree_relation :another_edb
            (betree_info
              (key_types STRING)
              (value_types INT)
              (betree_config 0.5 4 16 16)
              (betree_locator 0x3333444455556666777788 1000 4))))))
    (reads
      (output :foo :foo)
      (output :edb :edb)
      (output :multi_key :multi_key)
      (output :numeric_types :numeric_types)
      (output :temporal_data :temporal_data)
      (output :boolean_data :boolean_data)
      (output :key_only :key_only)
      (output :path_simple :path_simple)
      (output :path_specialized :path_specialized)
      (output :path_mixed :path_mixed)
      (output :path_empty :path_empty)
      (output :bar :bar)
      (output :another_edb :another_edb))))
