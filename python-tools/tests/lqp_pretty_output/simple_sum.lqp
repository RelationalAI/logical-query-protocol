(transaction
  (epoch
    (local_writes
      (define
        (fragment :f1
          (def :foo
            ([x::INT]
              (or
                (primitive :rel_primitive_eq x 1)
                (primitive :rel_primitive_eq x 2)
                (primitive :rel_primitive_eq x 3)))
            (attrs))
          (def :bar
            ([x::INT]
              (atom :foo x))
            (attrs))
          (def :result
            ([x::INT]
              (reduce
                ([a::INT b::INT c::INT]
                  (primitive :rel_primitive_add_monotype a b c))
                ([num::INT]
                  (atom :bar num))
                (terms x)))
            (attrs)))))
    (reads
      (output :foo :result))))