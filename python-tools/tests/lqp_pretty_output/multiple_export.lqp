(transaction
  (epoch
    (local_writes
      (define
        (fragment :f1
          (def :cola1
            ([row::INT v::INT]
              (and
                (primitive :rel_primitive_eq row 1)
                (primitive :rel_primitive_eq 2 v))))
          (def :cola2
            ([row::INT v::STRING]
              (and
                (primitive :rel_primitive_eq row 1)
                (primitive :rel_primitive_eq v "hello"))))
          (def :colb1
            ([row_1::STRING row_2::INT v::FLOAT]
              (and
                (primitive :rel_primitive_eq row_1 "b")
                (primitive :rel_primitive_eq row_2 12)
                (primitive :rel_primitive_eq v 6.1))))
          (def :colb2
            ([row_1::STRING row_2::INT v::INT]
              (and
                (primitive :rel_primitive_eq row_1 "b")
                (primitive :rel_primitive_eq row_2 12)
                (primitive :rel_primitive_eq v 123)))))))
    (reads
      (export
        (export_csv_config
          (path "/tmp/hello_lqp_1.csv")          (columns (column "col1name" :cola1) (column "col2name" :cola2))

          {
            :partition_size 10
            :compression ""
            :syntax_quotechar "\""
            :syntax_escapechar "\\"}))
      (export
        (export_csv_config
          (path "/tmp/hello_lqp_2.csv")          (columns (column "blahblah" :colb1) (column "foofoo" :colb2))

          {
            :syntax_quotechar "\""
            :syntax_escapechar "\\"})))))