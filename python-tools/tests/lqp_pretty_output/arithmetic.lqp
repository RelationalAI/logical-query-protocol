(transaction
  (epoch
    (writes
      (define
        (fragment :f1
          (def :decimal_64
            ([plus::(DECIMAL 18 6) minus::(DECIMAL 18 6) mult::(DECIMAL 18 6) div::(DECIMAL 18 6)]
              (exists [a::(DECIMAL 18 6) b::(DECIMAL 18 6)]
                (and
                  (relatom :dec_64_a a)
                  (relatom :dec_64_b b)
                  (primitive :rel_primitive_add_monotype a b plus)
                  (primitive :rel_primitive_subtract_monotype a b minus)
                  (primitive :rel_primitive_multiply_monotype a b mult)
                  (primitive :rel_primitive_divide_monotype a b div)))))
          (def :decimal_128
            ([plus::(DECIMAL 38 10) minus::(DECIMAL 38 10) mult::(DECIMAL 38 10) div::(DECIMAL 38 10)]
              (exists [a::(DECIMAL 38 10) b::(DECIMAL 38 10)]
                (and
                  (relatom :dec_128_a a)
                  (relatom :dec_128_b b)
                  (primitive :rel_primitive_add_monotype a b plus)
                  (primitive :rel_primitive_subtract_monotype a b minus)
                  (primitive :rel_primitive_multiply_monotype a b mult)
                  (primitive :rel_primitive_divide_monotype a b div))))))))
    (reads
      (output :dec_64 :decimal_64)
      (output :dec_128 :decimal_128))))
