(transaction
  (epoch
    (local_writes
      (define
        (fragment :f1
          (def :output_decimal_addition
            ([a::DECIMAL128 b::DECIMAL128 c::DECIMAL128]
              (and
                (relatom :dec_a a)
                (relatom :dec_b b)
                (primitive :rel_primitive_add_monotype a b c))))
          (def :output_decimal_subtraction
            ([a::DECIMAL128 b::DECIMAL128 c::DECIMAL128]
              (and
                (relatom :dec_a a)
                (relatom :dec_b b)
                (primitive :rel_primitive_subtract_monotype a b c))))
          (def :output_decimal_multiplication
            ([a::DECIMAL128 b::DECIMAL128 c::DECIMAL128]
              (and
                (relatom :dec_a a)
                (relatom :dec_b b)
                (primitive :rel_primitive_multiply_monotype a b c))))
          (def :output_decimal_division
            ([a::DECIMAL128 b::DECIMAL128 c::DECIMAL128]
              (and
                (relatom :dec_a a)
                (relatom :dec_b b)
                (primitive :rel_primitive_divide_monotype a b c)))))))
    (reads
      (output :dec_add :output_decimal_addition)
      (output :dec_sub :output_decimal_subtraction)
      (output :dec_mult :output_decimal_multiplication)
      (output :dec_div :output_decimal_division))))
