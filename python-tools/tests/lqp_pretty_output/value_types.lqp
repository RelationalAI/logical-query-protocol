(transaction
  (epoch
    (local_writes
      (define
        (fragment :f1
          (def :output_decimal64
            ([v::DECIMAL(                64                 4)]
              (relatom :dec64 v)))
          (def :output_decimal128
            ([v::DECIMAL(                128                 10)]
              (relatom :dec128 v)))
          (def :output_date
            ([v::DATE]
              (relatom :date v)))
          (def :output_datetime
            ([v::DATETIME]
              (relatom :datetime v))))))
    (reads
      (output :dec64 :output_decimal64)
      (output :dec128 :output_decimal128)
      (output :date :output_date)
      (output :datetime :output_datetime))))
