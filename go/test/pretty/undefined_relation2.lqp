(transaction
  (configure
    { :ivm.maintenance_level "off"
      :semantics_version 0})
  (epoch
    (writes
      (define
        (fragment :f6632
          (def :hello
            ([v::INT]
              (primitive :rel_primitive_eq v 101)))))
      (define
        (fragment :f6631
          (def :output
            ([v::INT]
              (atom :hello v))))))
    (reads
      (output :epoch0 :output)))
  (epoch
    (writes
      (undefine :f6632))
    (reads
      (output :epoch1 :output))))
